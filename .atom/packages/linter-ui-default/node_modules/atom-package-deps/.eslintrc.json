{
  "extends": "steelbrain",
  "rules": {
    "no-param-reassign": "off",
    "no-duplicate-imports": "off",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off",
    "no-await-in-loop": "off"
  },
  "globals": {
    "BufferedProcess": true,
    "__steelbrain_package_deps": true
  }
}
