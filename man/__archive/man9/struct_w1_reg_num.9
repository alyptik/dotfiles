.SH "NAME"
struct_w1_reg_num \- broken out slave device id
.SH "SYNOPSIS"
.sp
.nf
struct w1_reg_num {
#if defined(__LITTLE_ENDIAN_BITFIELD)
  __u64 family:8;
  __u64 id:48;
  __u64 crc:8;
#elif defined(__BIG_ENDIAN_BITFIELD)
  __u64 crc:8;
  __u64 id:48;
  __u64 family:8;
#else
#error "Please fix <asm/byteorder\&.h>"
#endif
};  
.fi
