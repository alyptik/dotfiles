.SH "SYNOPSIS"
.HP \w'struct\ fc_vport\ *\ fc_vport_create('u
.BI "struct fc_vport * fc_vport_create(struct\ Scsi_Host\ *\ " "shost" ", int\ " "channel" ", struct\ fc_vport_identifiers\ *\ " "ids" ");"
