+	setarr+=(markdirs menucomplete monitor multibyte notify pathdirs pipefail)
+	setarr+=(promptsubst pushdignoredups pushdminus pushdtohome rematchpcre)
+	() for 1 { unsetopt "$1"; }  $unsetarr
+	() for 1 { setopt "$1"; } $setarr
