+e scr.color = 1
+; identify files when opened
+e file.id = 1
+; automatically add bookmarks (flags) to syms, strings..
+e file.flag = 1
+; perform basic code analysis
+e file.analyze = 1
+; entry breakpoint
+e dbg.bep = main
+; e dbg.bep = entry
+; e dbg.bep = loader
