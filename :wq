+hash -d zf="${ZDOTDIR:-$HOME/.zsh.d}/zfunctions"
+hash -d zc="${ZDOTDIR:-$HOME/.zsh.d}/completions"
+hash -d zp="${ZDOTDIR:-$HOME/.zsh.d}/plugins"
+hash -d znc="/var/lib/znc/.znc/moddata/log/alyptik/freenode/"
+hash -d zsh="$ZSH"
